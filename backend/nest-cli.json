{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "language": "ts",
  "root": "apps",
  "sourceRoot": "apps",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "tsconfig.build.json"
  },
